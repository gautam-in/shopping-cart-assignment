    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/index.js",
      "!src/index.js",
      "!src/types.js",
      "!src/serviceWorker.js",
      "!src/**/*.d.js"
    ],
   
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
