  "license": "MIT",
  "dependencies": {
    "canned": "^0.3.12",
    "react-error-boundary": "^3.1.4",
    "react-helmet-async": "^1.3.0"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "husky": "^8.0.1"
  }
}
