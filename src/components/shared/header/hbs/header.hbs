<div class="page-header__logo">
    <picture>
        <source media="(min-width:650px)" srcset="static/images/logo.png">
        <source media="(min-width:1200px)" srcset="static/images/logo_2x.png">
        <img src="static/images/logo.png" alt="sabka_bazaar">
    </picture>
</div>
<div class="page-header__nav">
    {{{nav}}}
</div>
<div class="page-header__cart">
    <div class="page-header__cart-user-actions">
        {{#each userActions}}
        <a href="#" route={{route}} id={{id}}>{{text}}</a>
        {{/each}}
    </div>
    {{{cart}}}
</div>