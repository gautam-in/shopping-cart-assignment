<div class="slideshow-container row">
	<div id="banners-container">
		{{#each banners as |value key|}}
			<div class="mySlides fade">
			 {{> atoms/a-img imageUrl=value.bannerImageUrl }}
			</div>
		{{/each}}
		<div class="carousal-dot tab" id="carousal-dot">
			{{#each banners  as |value key|}}
				  <a class="dot" onclick="carousel.currentSlide({{@index}});return false;" href=""></a> 
			{{/each}}
		</div>
	</div>
	<a class="prev" onclick="carousel.plusSlides(-1);return false;" href="">Prev</a>
	<a class="next" onclick="carousel.plusSlides(1);return false;"  href="">Next</a>
</div>
<div class="carousal-dot mob" id="carousal-dot">
	{{#each banners  as |value key|}}
		 <a class="dot" onclick="carousel.currentSlide({{@index}});return false;" href=""></a> 
	{{/each}}
</div>