{
    "presets": [ "@babel/preset-react", 
        ["@babel/preset-env", {
          "useBuiltIns": "entry"
        }], 
        ["env", 
            {"modules": false,
                "targets": {
                    "node": "current"
                }
                }
        ]
    ],
    "plugins": [  
        "@babel/plugin-proposal-class-properties",
        [
            "@babel/plugin-transform-runtime",{"loose":true,"regenerator": true}           
        ]
    ]
}